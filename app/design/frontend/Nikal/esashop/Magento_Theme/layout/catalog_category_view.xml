<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <move element="breadcrumbs" destination="page.bottom.container" after="-"/>
        <referenceContainer name="page.bottom.container">
            <referenceBlock name="product.collections1" remove="true"/>
            <block class="Magento\Cms\Block\Block" name="catalog_collection_1">
                <arguments>
                    <argument name="block_id" xsi:type="string" translate="true">catalog_collection_1</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
